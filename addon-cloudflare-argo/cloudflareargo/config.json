{
	"name": "Cloudflare Argo",
	"version": "2.2",
	"slug": "argocloudflare",
	"description": "Argo for Home Assistant",
	"url": "https://github.com/wlatic/hassio.addons/blob/master/addon-cloudflare-argo/README.md",
	"ingress": false,
	"panel_icon": "mdi:docker",
	"homeassistant": "0.104.0",
	"arch": [
		"aarch64",
		"amd64",
		"armhf",
		"armv7"
	],
	"map": [
		"config:rw"
	],
	"startup": "services",
	"boot": "auto",
	"options": {
		"certificate": "/config/cert/argo.pem",
		"tunnel_name": "homeassistant",
		"hostname": "host",
		"service": "http://homeassistant:8123",
		"selfsigned": "False",
		"hostname2": "null",
		"service2": "null",
		"selfsigned2": "False",
		"hostname3": "null",
		"service3": "null",
		"selfsigned3": "False",
		"addconfig": "null"
	},
	"schema": {
		"certificate": "str",
		"tunnel_name": "str",
		"hostname": "str",
		"service": "str",
		"selfsigned": "bool",
		"hostname2": "str",
		"service2": "str",
		"selfsigned2": "bool",
		"hostname3": "str",
		"service3": "str",
		"selfsigned3": "bool",
		"addconfig": "str"
	}
}
