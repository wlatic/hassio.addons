{
	"name": "Cloudflare Argo",
	"version": "0.7",
	"slug": "argocloudflare",
	"description": "Argo for Home Assistant",
	"url": "https://github.com/royvinz/hassio.addons/blob/master/cloudflareargo/readme.md",
	"ingress": false,
	"panel_icon": "mdi:docker",
	"homeassistant": "0.104.0",
	"arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
	"map": [
         "config:rw"
  ],

	"startup": "services",
	"boot": "auto",
	"options": {
		
			"hostname": "hostname",
			"url": "http://hassio:8123",
			"certificate": "/config/cert/argo.pem"
	},
	"schema": false
}
